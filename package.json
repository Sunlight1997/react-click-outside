{
  "name": "react-click-outside",
  "version": "1.0.0",
  "description": "ClickOutside component for React",
  "keywords": [
    "react",
    "click",
    "outside",
    "component"
  ],
  "main": "build/index.js",
  "author": "TJ Holowaychuk <tj@tjholowaychuk.com>",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel -d build index.js",
    "start": "npm run serve & npm run watch",
    "watch": "watchify example/index.js -p browserify-hmr -o example/bundle.js -dv",
    "serve": "ecstatic -p 3000 example"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.5",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "browserify-hmr": "^0.3.1",
    "ecstatic": "^1.4.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "watchify": "^3.7.0"
  }
}
